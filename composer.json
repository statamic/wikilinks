{
    "name": "statamic/wikilinks",
    "autoload": {
        "psr-4": {
            "Statamic\\Wikilinks\\": "src"
        }
    },
    "require": {
        "statamic/cms": "^5.0 || ^6.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Wikilinks",
            "description": "Wikilinks addon"
        },
        "laravel": {
            "providers": [
                "Statamic\\Wikilinks\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}